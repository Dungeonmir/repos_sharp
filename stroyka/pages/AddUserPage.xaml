<Page x:Class="stroyka.pages.AddUserPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:stroyka.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddUserPage">

    <StackPanel>
        <Frame x:Name="frame"></Frame>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="btnBack" Height="36" Width="120" Margin="10,0,10,0" Click="btnBack_Click">Назад</Button>
            <Button x:Name="btnAddUser" Height="36" Width="120" Margin="10,0,10,0" Click="btnAddUser_Click">Добавить</Button>
        </StackPanel>

    </StackPanel>
</Page>
