<Page x:Class="stroyka.pages.AddMaterialPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:stroyka.pages"
      mc:Ignorable="d" 
      d:DesignHeight="580" d:DesignWidth="1200"
      Title="AddMaterialPage">

    <StackPanel>
        <Frame x:Name="frame" MaxHeight="480"></Frame>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Button x:Name="btnBack" Click="btnBack_Click" Margin="10,0,10,0" HorizontalAlignment="Center" VerticalAlignment="Center">Назад</Button>
            <Button x:Name="addBtn" Click="addBtn_Click" Margin="10,0,10,0" HorizontalAlignment="Center" VerticalAlignment="Center">Добавить</Button>
        </StackPanel>

    </StackPanel>
</Page>
